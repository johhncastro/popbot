{
  "name": "popbot-discord",
  "version": "1.0.0",
  "description": "A Discord bot with crypto price commands for Active Developer badge",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "deploy:guild": "tsx src/deploy-commands.ts --guild",
    "deploy:global": "tsx src/deploy-commands.ts --global"
  },
  "keywords": [
    "discord",
    "bot",
    "crypto",
    "prices",
    "slash-commands"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "discord-api-types": "^0.37.61",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
